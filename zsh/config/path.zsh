# Golang setting
export GOROOT="/usr/local/opt/go/libexec"
export GOPATH="$HOME/go"

# export coreutils path
export COREPATH="/usr/local/opt/coreutils/libexec/gnubin"

# export MANPATH="/usr/local/man:$MANPATH"
export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"
export ZPLUG_HOME="/usr/local/opt/zplug"

export RUBY_PATH="/usr/local/opt/ruby/bin"
# PATH
export PATH="/usr/local/sbin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$GOROOT/bin:$GOPATH/bin:$COREPATH:$RUBY_PATH"
